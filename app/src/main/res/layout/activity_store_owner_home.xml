<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="729dp"
    android:background="#515252"
    android:fillViewport="true"
    tools:context=".StoreOwnerHomeActivity"
    tools:layout_editor_absoluteX="-2dp"
    tools:layout_editor_absoluteY="1dp">

    <FrameLayout
        android:id="@+id/flFragmentHome"
        android:layout_width="409dp"
        android:layout_height="674dp"
        app:layout_constraintBottom_toTopOf="@+id/bottomNavigationViewHome"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0" />

    <com.google.android.material.bottomnavigation.BottomNavigationView
        android:id="@+id/bottomNavigationViewHome"
        android:layout_width="416dp"
        android:layout_height="76dp"
        android:background="@color/even_darker_grey"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:menu="@menu/bottom_nav_menu_store" />

</androidx.constraintlayout.widget.ConstraintLayout>

